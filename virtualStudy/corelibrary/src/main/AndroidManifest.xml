        <activity android:name=".B$3" android:launchMode="singleTop"></activity>
        <activity android:name=".B$4" android:launchMode="singleTop"></activity>
        <activity android:name=".B$5" android:launchMode="singleTop"></activity>
        <activity android:name=".B$6" android:launchMode="singleTop"></activity>
        <activity android:name=".B$7" android:launchMode="singleTop"></activity>
        <activity android:name=".B$8" android:launchMode="singleTop"></activity>

        <activity android:name=".C$1" android:launchMode="singleTask"></activity>
        <activity android:name=".C$2" android:launchMode="singleTask"></activity>
        <activity android:name=".C$3" android:launchMode="singleTask"></activity>
        <activity android:name=".C$4" android:launchMode="singleTask"></activity>
        <activity android:name=".C$5" android:launchMode="singleTask"></activity>
