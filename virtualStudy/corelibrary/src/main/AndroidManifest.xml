    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <activity android:name=".A$1" android:launchMode="standard"></activity>
        <activity android:name=".A$2" android:launchMode="standard" android:theme="@android:style/Theme.Translucent"></activity>

        <activity android:name=".B$1" android:launchMode="singleTop"></activity>
